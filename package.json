        "babel-jest": "^20.0.3",
        "babel-preset-es2015": "^6.24.1",
        "documentation": "^5.3.1",
        "eslint": "^4.6.0",
        "eslint-config-google": "^0.9.1",
        "jest": "^20.0.4",
        "mockdate": "^2.0.2"
    }
}
